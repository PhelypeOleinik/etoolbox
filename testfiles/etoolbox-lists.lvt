\input regression-test
\RequirePackage{etoolbox}

\START

\OMIT
  \def\do#1{\detokenize{"#1"}}
\TIMO
\TESTEXP{\docsvlist}
  {%
    \docsvlist{a,b,c}%
    \NEWLINE
    \docsvlist{a,{b,c}}%
    \NEWLINE
    \docsvlist{a, {b,c}}%
    \NEWLINE
    \docsvlist{a,{b}, {c}}%
  }

\TESTEXP{\forcsvlist}
  {%
    \forcsvlist{\do}{a,b,c}%
    \NEWLINE
    \forcsvlist{\do}{a,{b,c}}%
    \NEWLINE
    \forcsvlist{\do}{a, {b,c}}%
    \NEWLINE
    \forcsvlist{\do}{a,{b}, {c}}%
  }

\END