\input regression-test
\RequirePackage{etoolbox}

\START

\OMIT
  \def\do#1{\detokenize{"#1"}}
\TIMO
\TESTEXP{\docsvlist}
  {%
    \docsvlist{a,b,c}%
    \NEWLINE
    \docsvlist{a,{b,c}}%
    \NEWLINE
    \docsvlist{a, {b,c}}%
    \NEWLINE
    \docsvlist{a,{b}, {c}}%
    \NEWLINE
    \docsvlist{a,{b},,{c},}%
  }

\TESTEXP{\forcsvlist}
  {%
    \forcsvlist{\do}{a,b,c}%
    \NEWLINE
    \forcsvlist{\do}{a,{b,c}}%
    \NEWLINE
    \forcsvlist{\do}{a, {b,c}}%
    \NEWLINE
    \forcsvlist{\do}{a,{b}, {c}}%
    \NEWLINE
    \forcsvlist{\do}{a,{b},,{c},}%
  }

\OMIT
  \listadd{\testlist}{a}
  \listadd{\testlist}{{b}}
  \listadd{\testlist}{ {c}}
  \listadd{\testlist}{{d} }
  \listadd{\testlist}{ {e} }
  \listadd{\testlist}{}
  \listadd{\testlist}{ f }
\TIMO
\TESTEXP{\forlistloop}
  {%
    \forlistloop{\do}{\testlist}%
  }

\END